        </rdfs:subClassOf>
        <rdfs:comment>The profile for a Tool product.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.imsglobal.org/vocab/lti/v2/lti#ToolProxy -->

    <owl:Class rdf:about="&lti;ToolProxy">
        <rdfs:subClassOf rdf:resource="&lti;ToolSettingsContainer"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&lti;tool_profile"/>
                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&lti;tool_proxy_guid"/>
                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&lti;tool_consumer_profile"/>
                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&lti;lti_version"/>
