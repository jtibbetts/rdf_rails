    <owl:Class rdf:about="&lti;ToolSettingsContainer">
        <rdfs:comment>&lt;code&gt;ToolSettingsContainer&lt;/code&gt; is the base class for all resources that encapsulate tool settings.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.imsglobal.org/vocab/lti/v2/lti#Vendor -->

