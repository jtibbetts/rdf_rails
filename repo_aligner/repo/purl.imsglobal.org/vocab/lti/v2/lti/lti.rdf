        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&lti;security_contract"/>
                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>This entity defines the integration contract between a ToolConsumer and a Tool.   It is called a &quot;ToolProxy&quot; because its primary purpose is to provide the ToolConsumer with all of the information needed to embed a given Tool within the ToolConsumer system.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.imsglobal.org/vocab/lti/v2/lti#ToolProxyBinding -->

    <owl:Class rdf:about="&lti;ToolProxyBinding">
        <rdfs:subClassOf rdf:resource="&lti;ToolSettingsContainer"/>
