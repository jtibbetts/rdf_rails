  [ a owl:Restriction ;
    owl:onProperty      lti:service_provider;
    rdfs:comment "This is the service provider that hosts the deployed product."
  ] ,
  [ a owl:Restriction ;
    owl:onProperty      lti:product_info;
    owl:minCardinality  1;
    rdfs:comment "This is metadata about the product described in the profile."
  ] ,
  [ a owl:Restriction ;
    owl:onProperty      lti:profile;
    rdfs:comment "A resource that presents a view of this ProductInstance to one or more integration partners."
  ] ,
  [ a owl:Restriction ;
    owl:onProperty      lti:support;
    owl:allValuesFrom  lti:Contact;
    rdfs:comment "Contact information for support on this deployed instance of the product (Tool or ToolConsumer)."
  ] ,
  [ a owl:Restriction ;
