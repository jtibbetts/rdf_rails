  rdfs:subClassOf 
  [ a owl:Restriction ;
    owl:onProperty      lti:code;
    owl:minCardinality  1;
    rdfs:comment "A unique identifier for the resource."
  ] ; 
  rdfs:comment "A resource that can be identified by a <code>code</code> property.  The code property may be globally unique (as in the case of a Vendor resource) or locally unqiue (as in the case of a Product Family, where the code is locally unique within the scope of the parent Vendor resource)." .

lti:RestService rdf:type owl:Class ;
  rdfs:subClassOf lti:ServiceDescriptor ,
  [ a owl:Restriction ;
    owl:onProperty      lti:format;
    owl:minCardinality  1;
    rdfs:comment "The content type (also known as media type) of resources accessed via the endpoint defined by this RestService descriptor.   The endpoint of a REST service may support more than one content type if content negotiation is enabled as described in the <a href=\"http://www.w3.org/Protocols/rfc2616/rfc2616-sec12.html\">HTTP 1.1</a> specification."
  ] ,
  [ a owl:Restriction ;
    owl:onProperty      lti:endpoint;
    owl:minCardinality  1;
    rdfs:comment "A URI template that defines the REST endpoint for resources that may be accessed via the REST service.  The URI template should conform to the syntax in the proposed IETF standard [<a href=\"http://www.ietf.org/id/draft-gregorio-uritemplate-08.txt\">draft-gregorio-uritemplate-08</a>]. "
  ] ,
  [ a owl:Restriction ;
    owl:onProperty      lti:action;
    owl:allValuesFrom  lti:HttpMethod;
    owl:minCardinality  1;
    rdfs:comment "Specifies an HTTP action that is supported at the REST endpoint.  A given endpoint may support more than one action.  For example, it is common practice for a collection endpoint to accept POST requests, and a different item endpoint to accept GET, PUT, and DELETE requests."
  ] ; 
  rdfs:comment "A descriptor for a REST service." .

lti:ProductInstance rdf:type owl:Class;
  rdfs:subClassOf 
  [ a owl:Restriction ;
    owl:onProperty      lti:guid;
