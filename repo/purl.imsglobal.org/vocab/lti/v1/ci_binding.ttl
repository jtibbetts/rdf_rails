@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix bind: <http://purl.org/semantictools/v1/vocab/bind#> .
@prefix vann: <http://purl.org/vocab/vann#> .
@prefix ci: <http://purl.imsglobal.org/vocab/lti/v1/ci#> .

ci:ContentItemPlacement rdfs:subClassOf
  [ rdf:type bind:AssociationQualifier ;
    bind:onProperty ci:placementOf
  ], 
  [ rdf:type bind:AssociationQualifier ;
    bind:onProperty ci:presentation_document_target
  ] .
ci:ContentItem rdf:type bind:Addressable .
ci:ContentItem rdf:type bind:AbstractClass .
ci:ContentItem rdfs:subClassOf
  [ rdf:type bind:AssociationQualifier ;
    bind:onProperty ci:thumbnail
  ], 
  [ rdf:type bind:AssociationQualifier ;
    bind:onProperty ci:icon
  ] .
ci:Image rdf:type bind:Addressable .
ci:FileItem rdf:type bind:Addressable .

