###  http://localhost/purl.imsglobal.org/vocab/lis/v2/outcomes#comment

liso:comment rdf:type owl:ObjectProperty ;
             
             rdfs:comment "A comment about this Result suitable for display to the learner.  Typically, this is a comment made by the instructor or grader." ;
             
             rdfs:domain liso:Result ;
             
             rdfs:range lti-type:DataValue.Type .



###  http://localhost/purl.imsglobal.org/vocab/lis/v2/outcomes#lineItemOf

liso:lineItemOf rdf:type owl:ObjectProperty ;
                
                rdfs:comment "The context to which this LineItem belongs." ;
                
                rdfs:domain liso:LineItem ;
                
                rdfs:range lti:Context ;
                
                owl:inverseOf lti:lineItem .



###  http://localhost/purl.imsglobal.org/vocab/lis/v2/outcomes#reportingMethod

liso:reportingMethod rdf:type owl:ObjectProperty ;
                     
                     rdfs:comment "Identifies the property that is reported as the <code>resultScore</code> of the Results within this LineItem." ;
                     
                     rdfs:domain liso:LineItem ;
                     
                     rdfs:range rdf:Property .



###  http://localhost/purl.imsglobal.org/vocab/lis/v2/outcomes#result

liso:result rdf:type owl:ObjectProperty ;
            
            rdfs:comment "A Result associated with this LineItem." ;
            
            rdfs:domain liso:LineItem ;
            
            rdfs:range liso:Result ;
            
            owl:inverseOf liso:resultOf .



###  http://localhost/purl.imsglobal.org/vocab/lis/v2/outcomes#resultOf

liso:resultOf rdf:type owl:ObjectProperty ;
              
              rdfs:comment "The LineItem within which this Result is contained." ;
              
              rdfs:range liso:LineItem ;
              
              rdfs:domain liso:Result .



###  http://localhost/purl.imsglobal.org/vocab/lis/v2/outcomes#resultPerson

liso:resultPerson rdf:type owl:ObjectProperty ;
                  
                  rdfs:comment "The person whose score is recorded in this Result." ;
                  
                  rdfs:domain liso:Result ;
                  
                  rdfs:range lisp:LISPerson .



###  http://localhost/purl.imsglobal.org/vocab/lis/v2/outcomes#results

liso:results rdf:type owl:ObjectProperty ;
             
             rdfs:comment "The container holding the Results for this LineItem.  " ;
             
             rdfs:domain liso:LineItem ;
             
             rdfs:range liso:ResultContainer .



###  http://localhost/purl.imsglobal.org/vocab/lis/v2/outcomes#scoreConstraints

liso:scoreConstraints rdf:type owl:ObjectProperty ;
                      
                      rdfs:comment "Constraints on the scores recorded in the Results associated with this LineItem." ;
                      
                      rdfs:domain liso:LineItem ;
                      
                      rdfs:range liso:ScoreConstraints .



###  http://localhost/purl.imsglobal.org/vocab/lti/v2/lti#lineItem

lti:lineItem rdf:type owl:ObjectProperty .



